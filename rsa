#!/usr/bin/env python3
"""
Factorize an RSA number into prime factors.

Usage: rsa_factors <file>
where <file> is a file containing RSA numbers to factor.
One number per line.
"""

import sympy
import sys

def factorize_rsa_number(n):
    """Factorize an RSA number into prime factors."""
    factors = sympy.factorint(n)
    if len(factors) == 2:
        return tuple(factors.keys())
    else:
        return None

def main():
    """Read RSA numbers from input file and write prime factors to output."""
    if len(sys.argv) != 2:
        print("Usage: rsa_factors <file>")
        sys.exit(1)

    input_file = sys.argv[1]

    with open(input_file, 'r') as infile:
        for line in infile:
            n = int(line.strip())
            prime_factors = factorize_rsa_number(n)
            if prime_factors:
                print(f"{n} = {prime_factors[0]} * {prime_factors[1]}")

if __name__ == "__main__":
    main()
